    "N = int(input())\n",
    "time = input().split()\n",
    "for i in range(N):\n",
    "    time[i] = int(time[i])\n",
    "time.sort()\n",
    "total = 0\n",
    "for i in range(N):\n",
    "    temp = 0\n",
    "    for j in range(i+1):\n",
    "        temp += time[j]\n",
    "    total += temp\n",
    "print(total)"
   ]
